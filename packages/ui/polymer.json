{
  "entrypoint": "index.html",
  "shell": "lib/elements/app.js",
  "sources": [
   "lib/**/*.js",
   "manifest/**",
   "package.json",
   "manifest.json"
  ],
  "extraDependencies": [
    "env.js",
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**/*",
    "assets/**/*",
    "vendor/**/*",
    "shim/**/*"
  ],
  "moduleResolution": "node",
  "npm": true,
  "builds": [
    {
      "name": "pwa",
      "browserCapabilities": [
        "es2017"
      ],
      "js": {
        "minify": false,
        "compile": false,
        "transformModulesToAmd": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": false
    }
  ]
}
